<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.giasu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/giasu"
        android:label="@string/app_name"
        android:roundIcon="@drawable/giasu"
        android:supportsRtl="true"
        android:theme="@style/Theme.GiaSu">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name="com.example.android.Register.Register_User"
            android:exported="true" />
        <activity
            android:name="com.example.android.Register.Register_Teacher"
            android:exported="true" />
        <activity
            android:name="com.example.android.Register.Register"
            android:exported="true" />
        <activity
            android:name=".Login"
            android:exported="true" />
        <activity
            android:name=".Intro"
            android:exported="true" />
        <activity
            android:name=".Delay"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>